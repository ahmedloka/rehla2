<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/whitecolor"
    >
    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/app_color"
        android:visibility="visible"
        >
        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:layout_weight="1"
            app:titleTextColor="@color/whitecolor"
            app:titleTextAppearance="@style/toolbarStyle"
            >
 
        </android.support.v7.widget.Toolbar>
    </android.support.design.widget.AppBarLayout>

    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/dime_20"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_marginStart="@dimen/dime_5"
        android:layout_marginEnd="@dimen/dime_5"
         >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <android.support.v7.widget.AppCompatImageView
                android:layout_width="@dimen/dime_26"
                android:layout_height="@dimen/dime_26"
                app:srcCompat="@drawable/ic_locked"
                android:layout_margin="@dimen/dime_5"
                />
        </android.support.v7.widget.LinearLayoutCompat>
        <com.NativeTech.rehla.Utills.MyTextView
            android:id="@+id/change_password"
            android:layout_width="0dp"
            android:layout_weight="0.85"
            android:layout_height="wrap_content"
            android:padding="@dimen/_10_height"
            android:text="@string/change_password"
            style="@style/tvStyleBold"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_gravity="center"
            android:textColor="@android:color/black"
            android:layout_margin="@dimen/_10_height"
            />

    </android.support.v7.widget.LinearLayoutCompat>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="@color/graycolor"
        />

    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_margin="@dimen/dime_5"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <android.support.v7.widget.AppCompatImageView
                android:layout_width="@dimen/dime_26"
                android:layout_height="@dimen/dime_26"
                app:srcCompat="@drawable/ic_lan"
                android:layout_margin="@dimen/dime_5"
                />
        </android.support.v7.widget.LinearLayoutCompat>
        <com.NativeTech.rehla.Utills.MyTextView
            android:id="@+id/lang"
            android:layout_width="0dp"
            android:layout_weight="0.85"
            android:layout_height="wrap_content"
            android:padding="@dimen/_10_height"
            style="@style/tvStyleBold"

            android:text="@string/change_language"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_gravity="center"
            android:textColor="@android:color/black"
            android:layout_margin="@dimen/_10_height"
            />
    </android.support.v7.widget.LinearLayoutCompat>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="@color/graycolor"
        />
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_margin="@dimen/dime_5"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <android.support.v7.widget.AppCompatImageView
                android:layout_width="@dimen/dime_26"
                android:layout_height="@dimen/dime_26"
                app:srcCompat="@drawable/ic_question"
                android:layout_margin="@dimen/dime_5"
                />
        </android.support.v7.widget.LinearLayoutCompat>
        <com.NativeTech.rehla.Utills.MyTextView
            android:id="@+id/faq"
            android:layout_width="0dp"
            android:layout_weight="0.85"
            android:layout_height="wrap_content"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_gravity="center"
            style="@style/tvStyleBold"

            android:textColor="@android:color/black"
            android:padding="@dimen/_10_height"
            android:text="@string/faq"

            android:layout_margin="@dimen/_10_height"
            />

    </android.support.v7.widget.LinearLayoutCompat>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="@color/graycolor"
        />
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_margin="@dimen/dime_5"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <android.support.v7.widget.AppCompatImageView
                android:layout_width="@dimen/dime_26"
                android:layout_height="@dimen/dime_26"
                app:srcCompat="@drawable/ic_contact"
                android:layout_margin="@dimen/dime_5"
                />
        </android.support.v7.widget.LinearLayoutCompat>
        <com.NativeTech.rehla.Utills.MyTextView
            android:id="@+id/contact_us"
            android:layout_width="0dp"
            android:layout_weight="0.85"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/dime_10"
            android:paddingStart="@dimen/dime_10"
            android:paddingEnd="@dimen/dime_10"
            android:paddingBottom="@dimen/dime_15"
            style="@style/tvStyleBold"
            android:includeFontPadding="false"
            android:text="@string/complaint_and_suggestion"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_gravity="center"
            android:textColor="@android:color/black"
            android:layout_margin="@dimen/_10_height"
            />

    </android.support.v7.widget.LinearLayoutCompat>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="@color/graycolor"
        />
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_margin="@dimen/dime_5"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <android.support.v7.widget.AppCompatImageView
                android:layout_width="@dimen/dime_26"
                android:layout_height="@dimen/dime_26"
                app:srcCompat="@drawable/ic_contact"
                android:layout_margin="@dimen/dime_5"
                />
        </android.support.v7.widget.LinearLayoutCompat>
        <com.NativeTech.rehla.Utills.MyTextView
            android:id="@+id/terms_condition"
            android:layout_width="0dp"
            android:layout_weight="0.85"
            android:layout_height="wrap_content"
             style="@style/tvStyleBold"
            android:includeFontPadding="false"
           android:paddingTop="@dimen/dime_10"
           android:paddingStart="@dimen/dime_10"
           android:paddingEnd="@dimen/dime_10"
           android:paddingBottom="@dimen/dime_15"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_gravity="center"
            android:textColor="@android:color/black"
            android:text="@string/terms_and_conditions"

            android:layout_margin="@dimen/_10_height"

            />

    </android.support.v7.widget.LinearLayoutCompat>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="@color/graycolor"
        />
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="bottom|center"
        android:gravity="bottom|center"
        android:layout_marginBottom="@dimen/_10_height"
        android:orientation="vertical"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_gravity="center"
            >
            <com.NativeTech.rehla.Utills.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/developed_by"
                android:paddingStart="@dimen/dime_5"
                android:paddingEnd="@dimen/dime_5"
                android:textAppearance="@style/TextAppearance.Compat.Notification.Time"
                android:textAllCaps="false"
                android:textColor="@color/bluf"
                 />
            <com.NativeTech.rehla.Utills.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/native_tech"
                android:paddingStart="@dimen/dime_5"
                android:paddingEnd="@dimen/dime_5"
                android:textAllCaps="false"
                android:gravity="center"
                android:textAppearance="@style/TextAppearance.Compat.Notification.Time"

                android:textColor="@color/app_color"
                 />
        </android.support.v7.widget.LinearLayoutCompat>
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <com.NativeTech.rehla.Utills.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="@style/TextAppearance.Compat.Notification.Time"

                android:text="@string/naive_tech_info_gmail_com"
                android:paddingStart="@dimen/dime_5"
                android:paddingEnd="@dimen/dime_5"
                android:textAllCaps="false"
                android:textColor="@color/app_color"
                 />
        </android.support.v7.widget.LinearLayoutCompat>
    </android.support.v7.widget.LinearLayoutCompat>



</android.support.v7.widget.LinearLayoutCompat>
