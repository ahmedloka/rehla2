<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/_3sdp"
    android:layout_marginTop="@dimen/_4_height"
    android:layout_marginBottom="@dimen/_4_height"
    android:background="@drawable/button_shap_white"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="@dimen/_4_height"
            >
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_4_height"
                >
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/img"
                    android:layout_width="@dimen/profile_image_size"
                    android:layout_height="@dimen/profile_image_size"
                    android:layout_gravity="center"
                    />
            </android.support.v7.widget.LinearLayoutCompat>
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="1"
                android:layout_margin="@dimen/_4_height"
                >
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginStart="@dimen/half_safe_margin"
                    android:gravity="center"
                    >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/_4_height"
                        android:orientation="horizontal"

                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:orientation="horizontal"
                            >
                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:id="@+id/name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="mohamed sharabash"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:maxLines="1"
                                style="@style/tvStyleBold"
                                android:textColor="@color/blakcolor"
                                android:textSize="@dimen/_10ssp"
                                />
                            <android.support.v7.widget.AppCompatImageView
                                android:layout_width="@dimen/icon_size_small"
                                android:layout_height="@dimen/icon_size_small"
                                android:src="@drawable/ic_verified"
                                android:visibility="visible"
                                android:layout_marginStart="@dimen/half_safe_margin"
                                android:layout_marginEnd="@dimen/half_safe_margin"
                                android:layout_gravity="center"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.75"
                            android:layout_gravity="center"
                            android:orientation="horizontal"
                            >
                            <android.support.v7.widget.LinearLayoutCompat
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:visibility="gone"
                                android:orientation="horizontal"

                                >
                                <com.NativeTech.rehla.Utills.MyTextView
                                    android:id="@+id/car_type"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Honda Civic"
                                    android:layout_gravity="top"
                                    android:textAllCaps="false"
                                    android:textColor="@color/tab_border"
                                    android:textSize="@dimen/_9ssp"
                                    />
                                <com.NativeTech.rehla.Utills.MyTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="|"
                                    android:paddingStart="@dimen/_4_height"
                                    android:paddingEnd="@dimen/margin_txt"
                                    android:layout_gravity="top"
                                    android:textAllCaps="false"
                                    android:textStyle="bold"
                                    android:textColor="@color/tab_border"
                                    android:textSize="@dimen/_12ssp"
                                    />

                                <com.NativeTech.rehla.Utills.MyTextView
                                    android:id="@+id/car_color"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="white"
                                    android:layout_gravity="top"
                                    android:textAllCaps="false"
                                    android:textColor="@color/tab_border"
                                    android:textSize="@dimen/forgot_txt_size"
                                    />

                            </android.support.v7.widget.LinearLayoutCompat>

                        </android.support.v7.widget.LinearLayoutCompat>
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_weight="0.25"
                            android:visibility="gone"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="1"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/seat_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1"
                                android:layout_weight="0.4"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:textStyle="bold"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/forgot_txt_size"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:layout_width="0dp"
                                android:layout_weight="0.6"
                                android:layout_height="wrap_content"
                                android:text="@string/seat"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/forgot_txt_size"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>
                    <RatingBar
                        android:id="@+id/rate"
                        style="?android:attr/ratingBarStyleSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/app_color"
                        android:isIndicator="true"
                        android:numStars="5"
                        android:progressTint="@color/app_color"
                        android:rating="5"
                        android:layout_gravity="start"
                        android:stepSize="0.1"
                        />
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        >
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:id="@+id/reviews_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_0"
                            android:layout_gravity="top"
                            android:paddingStart="@dimen/_4_height"
                            android:paddingEnd="@dimen/_4_height"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/_12ssp"
                            />
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/reviews"
                            android:layout_gravity="top"
                            android:paddingStart="@dimen/_4_height"
                            android:paddingEnd="@dimen/_4_height"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/_10ssp"
                            />
                    </android.support.v7.widget.LinearLayoutCompat>

                </android.support.v7.widget.LinearLayoutCompat>
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.3"
                    android:weightSum="3"
                    android:orientation="vertical"
                    >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1.8"
                        android:orientation="vertical"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:weightSum="1"
                            >
                            <android.support.v7.widget.LinearLayoutCompat
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.5"
                                android:layout_gravity="center"
                                android:gravity="center"
                                >
                                <android.support.v7.widget.AppCompatImageView
                                    android:id="@+id/call"
                                    android:layout_width="@dimen/icon2_size"
                                    android:layout_height="@dimen/icon2_size"
                                    android:gravity="center"
                                    android:layout_gravity="center"
                                    android:src="@drawable/ic_phone_green"

                                    />
                            </android.support.v7.widget.LinearLayoutCompat>
                            <android.support.v7.widget.LinearLayoutCompat
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.5"
                                android:layout_gravity="center"
                                android:gravity="center"
                                >
                                <android.support.v7.widget.AppCompatImageView
                                    android:id="@+id/chat"
                                    android:layout_width="@dimen/icon2_size"
                                    android:layout_height="@dimen/icon2_size"
                                    android:gravity="center"
                                    android:layout_gravity="center"
                                    android:src="@drawable/ic_chat_green"

                                    />
                            </android.support.v7.widget.LinearLayoutCompat>
                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>
                    <com.NativeTech.rehla.Utills.MyTextViewBold
                        android:id="@+id/cash"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1.2"
                        android:text="@string/cash"
                        android:layout_gravity="center"
                        android:gravity="center"
                        style="@style/tvStyleBold"
                        android:textStyle="bold"
                        android:textAllCaps="false"
                        android:textColor="@color/app_color"
                        android:textSize="@dimen/_10ssp"
                        />
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.LinearLayoutCompat>
        </android.support.v7.widget.LinearLayoutCompat>
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1"
            >
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_4_height"
                android:orientation="vertical"
                >
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1"
                    >
                    <!--<android.support.v7.widget.AppCompatImageView
                        android:layout_width="@dimen/icon_size_small"
                        android:layout_height="match_parent"
                        android:src="@drawable/ic_from_to"
                        />-->
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="0.05"
                        android:padding="@dimen/_4_height"
                        android:orientation="vertical">
                    </android.support.v7.widget.LinearLayoutCompat>

                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="0.07"
                        android:paddingTop="@dimen/_4_height"
                        android:paddingBottom="@dimen/_4_height"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:id="@+id/imageView"
                            android:layout_width="@dimen/icon_size_small"
                            android:layout_height="@dimen/icon_size_small"
                            android:background="@drawable/button_shap_circle_green"
                            />
                        <View
                            android:id="@+id/view"
                            android:layout_width="@dimen/view_height"
                            android:layout_height="0dp"
                            android:layout_weight="0.6"
                            android:background="@color/tab_border"
                            />

                        <android.support.v7.widget.AppCompatImageView
                            android:id="@+id/imageView2"
                            android:layout_width="@dimen/icon_size_small"
                            android:layout_height="@dimen/icon_size_small"
                            android:background="@drawable/button_shap_circle_orange" />
                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_weight="0.88"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_4_height"
                            android:layout_marginBottom="@dimen/_1dp_height"
                            android:orientation="horizontal"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/from_date"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="12:00 am"
                                android:layout_gravity="top"
                                android:paddingStart="@dimen/_10_height"
                                android:paddingEnd="@dimen/_10_height"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/from_address"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:text="Washington pearl road ,new York"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />



                        </android.support.v7.widget.LinearLayoutCompat>
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_4_height"
                            android:layout_marginTop="@dimen/_1dp_height"
                            android:orientation="horizontal"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/to_date"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="12:50 am"
                                android:layout_gravity="bottom"
                                android:paddingStart="@dimen/_10_height"
                                android:paddingEnd="@dimen/_10_height"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/to_address"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:text="Harison, East Network, new York"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />



                        </android.support.v7.widget.LinearLayoutCompat>


                    </android.support.v7.widget.LinearLayoutCompat>

                </android.support.v7.widget.LinearLayoutCompat>
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_txt"
                    android:layout_margin="@dimen/margin_txt"
                    android:layout_marginBottom="@dimen/margin_txt"
                    android:weightSum="4"
                    >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.2"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingStart="@dimen/_10_height"
                            android:paddingEnd="@dimen/_10_height"
                            >

                            <com.NativeTech.rehla.Utills.MyTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/date"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/txt_tab_size"
                                />

                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:id="@+id/date"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="23 Feb, 18"
                                android:textStyle="bold"
                                android:textAllCaps="false"
                                style="@style/tvStyleBold"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/forgot_txt_size"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>

                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.6"
                        android:visibility="gone"
                        android:gravity="center"
                        android:orientation="horizontal"
                        >

                        <com.NativeTech.rehla.Utills.MyTextViewBold
                            android:id="@+id/price_all"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="70"
                            android:textStyle="bold"
                            style="@style/tvStyleBold"
                            android:textAllCaps="false"
                            android:paddingStart="@dimen/_4_height"
                            android:paddingEnd="@dimen/_4_height"
                            android:textColor="@color/app_color"
                            android:textSize="@dimen/forgot_txt_size"
                            />
                        <com.NativeTech.rehla.Utills.MyTextViewBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/sar"
                            android:textStyle="bold"
                            style="@style/tvStyleBold"
                            android:paddingStart="@dimen/_4_height"
                            android:paddingEnd="@dimen/_4_height"
                            android:textAllCaps="false"
                            android:textColor="@color/app_color"
                            android:textSize="@dimen/_12ssp"
                            />

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.6"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:visibility="visible"
                        android:gravity="center"
                        >
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/seat"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/txt_tab_size"
                            />

                        <com.NativeTech.rehla.Utills.MyTextViewBold
                            android:id="@+id/seat_number"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_0"
                            style="@style/tvStyleBold"
                            android:textStyle="bold"
                            android:textAllCaps="false"
                            android:textColor="@color/app_color"
                            android:textSize="@dimen/forgot_txt_size"
                            />

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        >

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:id="@+id/canceled_ride"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:visibility="gone"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            android:orientation="horizontal"
                            android:weightSum="1"
                            >
                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:id="@+id/num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:textStyle="bold"
                                style="@style/tvStyleBold"
                                android:paddingStart="@dimen/margin_txt"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/forgot_txt_size"
                                />
                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/seat"
                                android:paddingStart="@dimen/margin_txt"
                                style="@style/tvStyleBold"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/forgot_txt_size"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:visibility="gone"
                        >

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.9"
                        android:orientation="vertical"
                        android:layout_gravity="end|center"
                        android:gravity="end|center"
                        >
                        <com.NativeTech.rehla.Utills.ButtonBold
                            android:id="@+id/reject"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30_height"
                            android:textAllCaps="false"
                            style="?android:attr/borderlessButtonStyle"
                            android:textSize="@dimen/forgot_txt_size"
                            android:visibility="visible"
                            android:textColor="@color/whitecolor"
                            android:text="@string/reject"
                            android:background="@drawable/ride_status_red2"
                            />
                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.2"
                        android:orientation="vertical"
                        >

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.9"
                        android:orientation="vertical"
                        android:visibility="visible"
                        android:layout_gravity="end|center"
                        android:gravity="end"
                        >
                        <com.NativeTech.rehla.Utills.ButtonBold
                            android:id="@+id/accept"
                            style="?android:attr/borderlessButtonStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30_height"
                            android:textAllCaps="false"
                            android:textSize="@dimen/forgot_txt_size"
                            android:textColor="@color/whitecolor"
                            android:text="@string/accept"
                            android:background="@drawable/ride_status_green_2"
                            />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.LinearLayoutCompat>
        </android.support.v7.widget.LinearLayoutCompat>
    </LinearLayout>
</android.support.v7.widget.LinearLayoutCompat>