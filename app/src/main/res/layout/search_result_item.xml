<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
     android:layout_marginTop="@dimen/dime_5"
    android:layout_marginBottom="@dimen/dime_5"
    android:background="@drawable/button_shap_white"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="@dimen/dime_5"
            >
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/dime_5"
                >
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/img"
                    android:layout_width="@dimen/profile_image_size"
                    android:layout_height="@dimen/profile_image_size"
                    android:layout_gravity="center"
                    />
            </android.support.v7.widget.LinearLayoutCompat>
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="1"
                >
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginStart="@dimen/_10_height"
                    android:gravity="center"
                    >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/dime_5"
                        android:orientation="horizontal"

                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.65"
                            android:orientation="horizontal"
                            android:weightSum="1"
                            >
                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:id="@+id/name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:maxLines="1"
                                android:textColor="@color/blakcolor"
                                android:textSize="@dimen/_9ssp"
                                />
                            <android.support.v7.widget.LinearLayoutCompat
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:gravity="center"
                                >
                                <android.support.v7.widget.AppCompatImageView
                                    android:layout_width="@dimen/_15_height"
                                    android:layout_height="@dimen/_15_height"
                                    app:srcCompat="@drawable/ic_verified"
                                    android:layout_marginStart="@dimen/_10_height"
                                    android:layout_marginEnd="@dimen/_10_height"
                                    android:layout_gravity="center"
                                    />
                            </android.support.v7.widget.LinearLayoutCompat>
                        </android.support.v7.widget.LinearLayoutCompat>
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_weight="0.35"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:weightSum="1"
                            >
                            <RatingBar
                                android:id="@+id/rate"
                                style="?android:attr/ratingBarStyleSmall"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:backgroundTint="@color/app_color"
                                android:isIndicator="true"
                                android:numStars="5"
                                android:progressTint="@color/app_color"
                                android:rating="5"
                                android:layout_gravity="start"
                                android:stepSize="0.1"
                                />


                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_weight="1"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.65"
                            android:layout_gravity="center"
                            android:orientation="horizontal"
                            >
                            <android.support.v7.widget.LinearLayoutCompat
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"

                                >
                                <com.NativeTech.rehla.Utills.MyTextView
                                    android:id="@+id/car_type"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Honda Civic"
                                    android:layout_gravity="top"
                                    android:textAllCaps="false"
                                    android:textColor="@color/tab_border"
                                    android:textSize="@dimen/_9ssp"
                                    />
                                <com.NativeTech.rehla.Utills.MyTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="|"
                                    android:paddingStart="@dimen/dime_5"
                                    android:paddingEnd="@dimen/dime_5"
                                    android:layout_gravity="top"
                                    android:textAllCaps="false"
                                    android:textStyle="bold"
                                    android:textColor="@color/tab_border"
                                    android:textSize="@dimen/_10ssp"
                                    />

                                <com.NativeTech.rehla.Utills.MyTextView
                                    android:id="@+id/car_color"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="white"
                                    android:layout_gravity="top"
                                    android:textAllCaps="false"
                                    android:textColor="@color/tab_border"
                                    android:textSize="@dimen/_9ssp"
                                    />

                            </android.support.v7.widget.LinearLayoutCompat>

                        </android.support.v7.widget.LinearLayoutCompat>
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="0dp"
                            android:layout_weight="0.35"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:weightSum="1"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/count_reviews"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/_0"
                                android:gravity="center"
                                android:layout_gravity="center"
                                android:paddingStart="@dimen/dime_5"
                                android:paddingEnd="@dimen/dime_5"
                                android:textStyle="bold"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_9ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/reviews"
                                android:layout_gravity="center"
                                android:paddingStart="@dimen/dime_5"
                                android:paddingEnd="@dimen/dime_5"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_9ssp"
                                />
                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_weight="0.35"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="1"
                        >
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:id="@+id/count_ride"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_0"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/_9ssp"
                            />
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/rides_offered"
                            android:layout_gravity="center"
                            android:paddingStart="@dimen/dime_5"
                            android:paddingEnd="@dimen/dime_5"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/_9ssp"
                            />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.LinearLayoutCompat>
        </android.support.v7.widget.LinearLayoutCompat>
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1"
            >
            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/dime_5"
                android:orientation="vertical"
                >
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1"
                    >
                    <!--<android.support.v7.widget.AppCompatImageView
                        android:layout_width="@dimen/icon_size_small"
                        android:layout_height="match_parent"
                        android:src="@drawable/ic_from_to"
                        />-->
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="0.05"
                        android:padding="@dimen/dime_5"
                        android:orientation="vertical">
                    </android.support.v7.widget.LinearLayoutCompat>

                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="0.07"
                        android:paddingTop="@dimen/dime_5"
                        android:paddingBottom="@dimen/dime_5"
                        android:orientation="vertical">
                        <android.support.v7.widget.AppCompatImageView
                            android:id="@+id/imageView"
                            android:layout_width="@dimen/icon_size_small"
                            android:layout_height="@dimen/icon_size_small"
                            android:background="@drawable/button_shap_circle_green"
                            />
                        <View
                            android:id="@+id/view"
                            android:layout_width="@dimen/view_height"
                            android:layout_height="0dp"
                            android:layout_weight="0.6"
                            android:background="@color/tab_border"
                            />

                        <android.support.v7.widget.AppCompatImageView
                            android:id="@+id/imageView2"
                            android:layout_width="@dimen/icon_size_small"
                            android:layout_height="@dimen/icon_size_small"
                            android:background="@drawable/button_shap_circle_orange" />
                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_weight="0.88"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dime_5"
                            android:layout_marginBottom="@dimen/dime_5"
                            android:orientation="horizontal"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/from_date"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="12:00 am"
                                android:layout_gravity="top"
                                android:paddingStart="@dimen/dime_5"
                                android:paddingEnd="@dimen/dime_5"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/from_address"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:text="Washington pearl road ,new York"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />



                        </android.support.v7.widget.LinearLayoutCompat>
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dime_5"
                            android:layout_marginTop="@dimen/dime_5"
                            android:orientation="horizontal"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/to_date"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="12:50 am"
                                android:layout_gravity="bottom"
                                android:paddingStart="@dimen/dime_5"
                                android:paddingEnd="@dimen/dime_5"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/to_address"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:text="Harison, East Network, new York"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_10ssp"
                                />



                        </android.support.v7.widget.LinearLayoutCompat>


                    </android.support.v7.widget.LinearLayoutCompat>

                </android.support.v7.widget.LinearLayoutCompat>
                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/dime_5"
                    android:weightSum="3"
                    >
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.65"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:gravity="center"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/date"
                                android:textAllCaps="false"
                                android:textColor="@color/tab_border"
                                android:textSize="@dimen/_9ssp"
                                />

                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/date"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="23 Feb, 18"
                                android:textStyle="bold"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/_9ssp"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>

                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.6"
                        android:visibility="gone"
                        android:gravity="center"
                        android:orientation="horizontal"
                        >

                        <com.NativeTech.rehla.Utills.MyTextViewBold
                            android:id="@+id/price_all"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="70"
                            android:textStyle="bold"
                            android:textAllCaps="false"
                            android:paddingStart="@dimen/dime_5"
                            android:textColor="@color/app_color"
                            android:textSize="@dimen/forgot_txt_size"
                            />
                        <com.NativeTech.rehla.Utills.MyTextViewBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/sar"
                            android:textStyle="bold"
                            android:paddingStart="@dimen/dime_5"
                            android:textAllCaps="false"
                            android:textColor="@color/app_color"
                            android:textSize="@dimen/forgot_txt_size"
                            />

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.75"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:visibility="visible"
                        android:gravity="center
"
                        >
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/seat_price"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/_9ssp"
                            />

                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_gravity="center"
                            android:gravity="center"
                            >
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:id="@+id/price"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/_0"
                                android:textStyle="bold"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/_9ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/sar"
                                android:layout_gravity="center"
                                android:paddingStart="@dimen/margin_txt"
                                android:paddingEnd="@dimen/margin_txt"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/_9ssp"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>
                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:visibility="visible"
                        android:gravity="center
"
                        >
                        <com.NativeTech.rehla.Utills.MyTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/available_seats"
                            android:textAllCaps="false"
                            android:textColor="@color/tab_border"
                            android:textSize="@dimen/_9ssp"
                            android:maxLines="1"
                            />

                        <com.NativeTech.rehla.Utills.MyTextView
                            android:id="@+id/seat_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textStyle="bold"
                            android:textAllCaps="false"
                            android:textColor="@color/app_color"
                            android:textSize="@dimen/_10ssp"
                            />

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:id="@+id/canceled_ride"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:visibility="gone"
                        >
                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            android:orientation="horizontal"
                            android:weightSum="1"
                            >
                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:id="@+id/num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:textStyle="bold"
                                android:paddingStart="@dimen/dime_5"
                                android:paddingEnd="@dimen/dime_5"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/_10ssp"
                                />
                            <com.NativeTech.rehla.Utills.MyTextViewBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/seat"
                                android:paddingStart="@dimen/dime_5"
                                android:paddingEnd="@dimen/dime_5"
                                android:layout_gravity="center"
                                android:textAllCaps="false"
                                android:textColor="@color/app_color"
                                android:textSize="@dimen/_10ssp"
                                />

                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:visibility="gone"
                        >

                    </android.support.v7.widget.LinearLayoutCompat>
                    <android.support.v7.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.75"
                        android:orientation="vertical"
                        android:visibility="visible"
                        android:layout_gravity="end"
                        android:gravity="center"
                        >
                        <com.NativeTech.rehla.Utills.ButtonBold
                            android:id="@+id/btn_request"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/button_height_size"
                            android:textAllCaps="false"
                            android:textSize="@dimen/_10ssp"
                            android:textColor="@color/whitecolor"
                            android:text="@string/request"
                            style="?android:attr/borderlessButtonStyle"
                            android:background="@drawable/ride_status_green_2"
                            />
                    </android.support.v7.widget.LinearLayoutCompat>
                </android.support.v7.widget.LinearLayoutCompat>
            </android.support.v7.widget.LinearLayoutCompat>
        </android.support.v7.widget.LinearLayoutCompat>
    </LinearLayout>
</LinearLayout>